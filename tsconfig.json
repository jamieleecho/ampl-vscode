{
    "exclude": [
        "node_modules",
        "out",
        "./node_modules",
        "node_modules/*",
    ],
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "es6"
        ],
        "skipLibCheck": true,
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
    },
}